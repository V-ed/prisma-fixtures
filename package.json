{
  "name": "prisma-fixtures",
  "version": "0.0.1",
  "description": "Prisma 2 fixture loading paradigm",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "npm-run-all -s build:ts build:ts-alias",
    "build:ts": "tsc",
    "build:ts-alias": "tsc-alias",
    "format": "eslint --fix \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "format:tests": "eslint --fix \"tests/**/*.ts\"",
    "lint:tests": "eslint \"tests/**/*.ts\"",
    "test": "jest --detectOpenHandles --silent",
    "coveralls": "jest --detectOpenHandles --silent --coverage && cat ./coverage/lcov.info | coveralls",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/V-ed/prisma-fixtures.git"
  },
  "keywords": [],
  "author": {
    "name": "Guillaume Marcoux",
    "url": "https://github.com/V-ed"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/V-ed/prisma-fixtures/issues"
  },
  "homepage": "https://github.com/V-ed/prisma-fixtures#readme",
  "devDependencies": {
    "@types/jest": "27.0.0",
    "@types/node": "16.6.0",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "coveralls": "3.1.1",
    "eslint": "7.32.0",
    "eslint-plugin-jest": "24.4.0",
    "jest": "27.0.6",
    "jest-extended": "0.11.5",
    "json5": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "27.0.4",
    "ts-node": "10.2.0",
    "tsc-alias": "^1.3.8",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@prisma/client": "^2.14.0",
    "class-importer": "0.0.1"
  },
  "peerDependencies": {
    "@prisma/client": "^2.14.0"
  }
}
