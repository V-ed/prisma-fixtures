{
  "name": "prisma-fixtures",
  "version": "0.1.0",
  "description": "Prisma fixture loading paradigm",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "npm-run-all lint:*",
    "lint:src": "eslint \"src/**/*.ts\"",
    "lint:tests": "eslint \"tests/**/*.ts\"",
    "format": "npm-run-all format:*",
    "format:src": "eslint --fix \"src/**/*.ts\"",
    "format:tests": "eslint --fix \"tests/**/*.ts\"",
    "test": "vitest run --config tests/vitest.config.ts"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/V-ed/prisma-fixtures.git"
  },
  "keywords": [],
  "author": {
    "name": "Guillaume Marcoux",
    "url": "https://github.com/V-ed"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/V-ed/prisma-fixtures/issues"
  },
  "homepage": "https://github.com/V-ed/prisma-fixtures#readme",
  "prettier": "@v-ed/prettier-config",
  "eslintConfig": {
    "extends": [
      "@v-ed/eslint-config"
    ]
  },
  "devDependencies": {
    "@types/node": "16.11.33",
    "@v-ed/eslint-config": "0.1.0",
    "@v-ed/prettier-config": "0.1.1",
    "@v-ed/tsconfig": "0.1.2",
    "c8": "7.11.2",
    "eslint": "8.14.0",
    "npm-run-all": "4.1.5",
    "typescript": "4.6.4",
    "vite-tsconfig-paths": "3.4.1",
    "vitest": "0.10.1"
  },
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "class-importer": "^0.1.1"
  }
}
